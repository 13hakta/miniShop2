		<composite alias="Files" class="msProductFile" local="id" foreign="product_id" cardinality="many" owner="local" />
		<composite alias="Categories" class="msCategoryMember" local="id" foreign="product_id" cardinality="many" owner="local" />
		<aggregate alias="Product" class="msProduct" local="id" foreign="id" cardinality="one" owner="foreign" />
		<aggregate alias="Vendor" class="msVendor" local="vendor" foreign="id" cardinality="one" owner="foreign" />
