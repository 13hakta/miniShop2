<?xml version="1.0" encoding="UTF-8"?>
<model package="minishop2" baseClass="xPDOObject" platform="mysql" defaultEngine="MyISAM" phpdoc-package="minishop2" phpdoc-subpackage="" version="1.1">


	<object class="msCategory" extends="modResource">
